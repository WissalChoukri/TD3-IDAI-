#include <iostream>

template <typename T>
T carre(const T& valeur) {
    return valeur * valeur;
}

int main() {
    int entier = 5;
    double reel = 3.14;

    int carreEntier = carre(entier);
    double carreReel = carre(reel);

    std::cout << "Carré de l'entier : " << carreEntier << std::endl;
    std::cout << "Carré du nombre réel : " << carreReel << std::endl;

    return 0;
}


